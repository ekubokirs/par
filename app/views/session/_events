<% @events.each do |event| %>
	<table class="row">
		<thead>
			<tr>
				<td>Event Name</td>
				<td>Event Type</td>
				<td>Edit</td>
				<td>Delete</td>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td><%= link_to event.name, event_url(event.id) %></td>
				<td><%= event.type %></td>
				<td><%= link_to "Edit", edit_event_url(event.id) %></td>
				<td><%= link_to "Delete", event_url(event.id), method: :delete %></td>
			</tr>
		</tbody>
	</table>
<% end %>